#!groovy
// Check URL properties
properties([disableConcurrentBuilds()])

pipeline {
	agent any
	options {
		buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
	}
	stages {
		stage("First step") {
			steps {
				sh 'ping mail.ru/'
			}
		}
		stage("Second step") {
			steps {
				sh 'ping https://mtkp.bmstu.ru/'
			}
		}
	}
}
